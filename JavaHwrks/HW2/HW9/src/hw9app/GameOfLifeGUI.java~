    //Modify Family
    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton4ActionPerformed
        String name = jTextField2.getText();
        if(name.isEmpty()){ //Give warning if text field is empty
            jLabel9.setText("Write Something!");
            jLabel9.setForeground(Color.red);
        }
        if(jList1.isSelectionEmpty()){ //Give warning if none famil selected
            jLabel9.setText("Select a Family!");
            jLabel9.setForeground(Color.red);
        }
        //Modify Family
        if(!name.isEmpty() && !jList1.isSelectionEmpty()){
            int index =jList1.getSelectedIndex();
            getjList1().setModel(Family);        
            Family.set(index, name);
            jTextField2.setText("");
        }
    }//GEN-LAST:event_jButton4ActionPerformed
    //Family END **********************************************************************  
    
    //Add person
    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton5ActionPerformed
        
        String name = jTextField3.getText(); //Read person Name
        if(name.isEmpty()){
            jLabel5.setText("Write Something!");
            jLabel5.setForeground(Color.red);
        }
        //Do this if list is selected
        if(!name.isEmpty() && !jList1.isSelectionEmpty())
        {
            DefaultListModel model = new DefaultListModel();
            model = (DefaultListModel) jList2.getModel();
            jList2.setModel(model);
            model.insertElementAt(name,model.size());
            //Label*********************************/
            jLabel3.setText("Person : "+ model.getSize());
            jLabel3.setForeground(Color.blue);
            jTextField3.setText("");
            jLabel5.setText("");
            /***************************************/
        }
        //give a warning if no family selected
        if(jList1.isSelectionEmpty()){
            jLabel5.setText("Select a Family!");
            jLabel5.setForeground(Color.red);
        }
    }//GEN-LAST:event_jButton5ActionPerformed

    //Remove a person
    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        //A Person must be selected else its gonna get exception
        if(jList2.isSelectionEmpty()){
            jLabel6.setText("Select a Person!");
            jLabel6.setForeground(Color.red);
        }
        //A Family must be selected else its gonna get exception
        if(jList1.isSelectionEmpty()){
            jLabel6.setText("Select a Family!");
            jLabel6.setForeground(Color.red);
        }
        //Do this if both lists are selected
        if(!jList2.isSelectionEmpty() && !jList1.isSelectionEmpty()){ 
            int index = jList1.getSelectedIndex();
            int index2 = jList2.getSelectedIndex();
            // Remove the selected item
            Person[index].removeElementAt(index2);
            //Label***************************************/
            jLabel3.setText("Person : "+ Person[index].getSize());
            jLabel3.setForeground(Color.blue);
            jLabel6.setText("");
            /***************************************/
        }
        
        
    }//GEN-LAST:event_jButton1ActionPerformed

