    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton6ActionPerformed
       System.exit(0);
    }//GEN-LAST:event_jButton6ActionPerformed

    //Modify Person
    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton7ActionPerformed
        String name = jTextField4.getText();
        if(name.isEmpty()){ //Give warning if text field is empty
            jLabel5.setText("Write Something!");
            jLabel5.setForeground(Color.red);
        }
        if(jList2.isSelectionEmpty()){ //Give warning if none famil selected
            jLabel5.setText("Select a Person!");
            jLabel5.setForeground(Color.red);
        }
        if(jList1.isSelectionEmpty()){ //Give Warning if no family selected
            jLabel5.setText("Select a Family!");
            jLabel5.setForeground(Color.red);
        }
        
        //Modify Person
        if(!name.isEmpty() && !jList2.isSelectionEmpty() && !jList1.isSelectionEmpty()){
            int index  = jList1.getSelectedIndex();
            int index2 = jList2.getSelectedIndex();
            //jList2.setModel(Person[index]);        
            Person[index].set(index2, name);
            jTextField4.setText("");
        }
    }//GEN-LAST:event_jButton7ActionPerformed

