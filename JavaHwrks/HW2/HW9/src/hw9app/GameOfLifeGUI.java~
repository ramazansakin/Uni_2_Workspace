    //Add Family
    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed
        
        String name = jTextField1.getText();
        if(name.isEmpty()){ //Give a warning
            jLabel9.setText("Write Something!");
            jLabel9.setForeground(Color.red);
        }
        if(!name.isEmpty()){ //Get family name if there is something in text field
            getjList1().setModel(Family);
            Family.insertElementAt(name, Family.size());
            
            //Label*********************************/
            counter++;
            jLabel2.setText("Families : "+ counter);
            jLabel2.setForeground(Color.blue);
            jTextField1.setText("");
            jLabel9.setText("");
            /***************************************/
        }
    }//GEN-LAST:event_jButton2ActionPerformed
    
    //Remove Family
    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed
        if(jList1.isSelectionEmpty()){
            jLabel7.setText("Select a Family!");
            jLabel7.setForeground(Color.red);
        }
        if(!jList1.isSelectionEmpty()){
            int index = jList1.getSelectedIndex();
            // Remove the selected item
            Family.removeElementAt(index);
            //DefaultListModel model = new DefaultListModel();
            //model = (DefaultListModel) jList2.getModel();
            Person[index].removeAllElements();
            for(int i = index;i<99;++i){
                Person[i] = Person[i+1];
            }
            //Label***************************************/
            counter--;
            jLabel2.setText("Families : "+ counter);
            jLabel2.setForeground(Color.blue);
            /***************************************/
        }
    }//GEN-LAST:event_jButton3ActionPerformed
    
