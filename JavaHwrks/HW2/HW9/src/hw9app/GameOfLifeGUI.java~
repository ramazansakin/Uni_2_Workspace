    //Create a list
    private DefaultListModel Family = new DefaultListModel(); //Family List
    private DefaultListModel [] Person = new DefaultListModel[100]; //Person List
    private static int counter = 0; //Number of family
    
    //fill person list
    //We should take memoery to use variables.
    //ListModelRight without assignment will not work
    public void fillList(){
        for(int i =0; i<100 ; ++i){
            DefaultListModel modell = new DefaultListModel();
            Person[i] = modell ;
        }
    }
    public void handlePersonList(){
        //Listen jList1
        jList1.addListSelectionListener(new ListSelectionListener() {
            @Override
            public void valueChanged(ListSelectionEvent e) {
                //throw new UnsupportedOperationException("Not supported yet."); //To change body of generated methods, choose Tools | Templates.
                if (e.getValueIsAdjusting() == false)
                {
                    int i = jList1.getSelectedIndex();
                    if(i<0) //to avoid exception
                        i++;
                    jList2.setModel(Person[i]);
                    jLabel3.setText("Person : "+ Person[i].getSize()); //Set Label
                    jLabel5.setText(""); //Set Label
                    jLabel6.setText(""); //Set Label
                    jLabel7.setText(""); //Set Label
                    jLabel9.setText(""); //Set Label
                }
                                
            }
        });
        
        //Listen jList2
        jList2.addListSelectionListener(new ListSelectionListener() {
            @Override
            public void valueChanged(ListSelectionEvent e) {
                //throw new UnsupportedOperationException("Not supported yet."); //To change body of generated methods, choose Tools | Templates.
                if (e.getValueIsAdjusting() == false){
                    jLabel6.setText(""); //Set Label
                }                         
            }
        });
    }
    
    //Getter to clear default List | called from Main
    public javax.swing.JList getjList1() {
        return jList1;
    }
    //Getter to clear default List | called from Main
    public javax.swing.JList getjList2() {
        return jList2;
    }
